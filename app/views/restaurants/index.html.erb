<%= render 'partials/header' %>
<div class="container">
  <div class="row">
    <% @restaurants.each do |restaurant| %>
      <div class="col-md-4">
        <%= image_tag restaurant.avatar.url %>
        <h1><%= restaurant.name %></h1>
        <p><%= restaurant.description %></p>
      </div>
    <% end %>
  </div>
</div>
